<!DOCTYPE html>
<html>
    <%- include('../partials/head', {title : 'Test'}) %>
    <body>
        <% include ../partials/body %>

        <h2>Index</h2>

        <h4>TestSettings</h4>
        <hr />
        <div class="row">
            <div class="col-md-4">
                <form>
                    <div class="form-group">
                        <label for="DepartFrom" class="control-label"></label>
                        <input name="DepartFrom" id="DepartFrom" class="form-control" value="<%= test.DepartFrom %>" />
                    </div>
                    <div class="form-group">
                        <label for="DepartOn" class="control-label"></label>
                        <input name="DepartOn" id="DepartOn" class="form-control" value="<%= test.DepartOn.toISOString().split('.')[0] %>" />
                    </div>
                    <div class="form-group">
                        <label for="ArriveAt" class="control-label"></label>
                        <input name="ArriveAt" id="ArriveAt" class="form-control" value="<%= test.ArriveAt %>" />
                    </div>
                    <div class="form-group">
                        <label for="ReturnOn" class="control-label"></label>
                        <input name="ReturnOn" id="ReturnOn" class="form-control" value="<%= test.ReturnOn.toISOString().split('.')[0] %>" />
                    </div>
                    <div class="form-group">
                        <label for="PurchasedOn" class="control-label"></label>
                        <input name="PurchasedOn" id="PurchasedOn" class="form-control" value="<%= test.PurchasedOn.toISOString().split('.')[0] %>" />
                    </div>
                </form>
            </div>
        </div>

        <% include ../partials/footer %>
    </body>
</html>

